{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "indent": "off",
        "no-unused-vars": "off",
        "import/prefer-default-export": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "import/extensions": "off",
        "consistent-return": "off",
        "func-names": "off",
        "max-len": "off",
        "camelcase": "off"
    }
}
